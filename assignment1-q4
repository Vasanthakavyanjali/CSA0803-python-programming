print(n vasantha kavyanjali, regno:192211609)
user_input = input("Enter a letter (A-Z): ")
if user_input.isalpha() and len(user_input) == 1:
    letter = user_input.upper()
    if letter == 'A':
        print("Apple")
    elif letter == 'B':
        print("Bat")
    elif letter == 'Z':
        print("Zip")
    else:
        print("Word not found for the entered letter.")
else:
    print("Invalid input. Please enter a single letter (A-Z).")
